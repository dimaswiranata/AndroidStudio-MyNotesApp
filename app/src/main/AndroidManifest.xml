<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mynotesapp.wiranata.com">
    <permission android:name="mynotesapp.wiranata.com.READ_DATABASE" android:protectionLevel="normal"/>
    <permission android:name="mynotesapp.wiranata.com.WRITE_DATABASE" android:protectionLevel="normal"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.NoteAddUpdateActivity">
        </activity>
        <provider
            android:authorities="mynotesapp.wiranata.com"
            android:name=".provider.NoteProvider"
            android:exported="true"
            android:readPermission="mynotesapp.wiranata.com.READ_DATABASE"
            android:writePermission="mynotesapp.wiranata.com.WRITE_DATABASE" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>